			<script>
				$(":radio[name='license']").on("change", function () {
					console.log("值改变了");
					var checked = $(this).val();
					$('#license_n input[name="official"]').prop("checked", "");
					$('#license_y input[name="official"]').prop("checked", "");
					if (checked == 'Y') {
						$("#license_n").hide();
						$("#license_y").show();
					} else {
						$("#license_y").hide();
						$("#license_n").show();
					}
				});

				//2s自动关闭提示
				function autoTips(txt, ms, fn) {
					var txt = txt.toString(),
						ms = ~~ms <= 10 ? 10 : ms,
						fn = typeof fn === 'function' ? fn :
						function () {},
						style =
						'display:none;position:fixed;z-index:999;left:50%;top:50%;width:150px;padding:20px 0;line-height:1.5;margin-top:-25px;margin-left:-60px;background:rgba(0,0,0,.6);border-radius:8px;text-align:center;color:#fff;',
						template = '<div class="autoTips" style="' + style + '">' +
						txt + '</div>';
					if ($('.autoTips').length < 1) {
						$(template).appendTo('body').fadeIn();
					} else {
						$('.autoTips').html(txt).fadeIn();
					}
					setTimeout(function () {
						$('.autoTips').fadeOut(fn);
					}, ms);
				};

				function btn_save() {
					var name = $("#name").val();
					var contact = $("#contact").val();
					var qq = $("#qq").val();
					var profession = $("#profession").val();
					//var remark = $("#remark").val();
					var license = $("input[name='license']:checked").val()
					var hasOA = '';
					var isCertified = '';
					var official = $("input[name='official']:checked").val()

					if (!name) {
						autoTips("请填写姓名");
						return;
					}
					if (!contact) {
						autoTips("请填写手机");
						return;
					}
					if (!(/^1(3|4|5|7|8)\d{9}$/.test(contact))) {
						autoTips("请输入正确的11位手机号码");
						return;
					}
					if (!qq) {
						autoTips("请填写qq号码");
						return;
					}
					if (!(/^\d{5,15}$/.test(qq))) {
						autoTips("请输入正确的qq号码");
						return;
					}
					if (!profession) {
						autoTips("请填写行业");
						return;
					}

					if (!license) {
						autoTips("请选择是否有营业执照");
						return;
					}

					if (!official) {
						autoTips("请选择有无公众号");
						return;
					}
					switch (official) {
						case '1':
						case 1:
							hasOA = 'Y';
							isCertified = 'Y';
							break;
						case '2':
						case 2:
							hasOA = 'Y';
							isCertified = 'N';
							break;
						case '3':
						case 3:
							hasOA = 'N';
							isCertified = 'N';
							break;
						default:
							autoTips("请选择有无公众号");
							return;
							break;
					}

					var data = {
						name: name,
						contact: contact,
						qq: qq,
						profession: profession,
						//remark: remark,
						license: license,
						official: official,
						hasOA: hasOA,
						isCertified: isCertified,
						source: 'page_about'
					}

					console.log(data)

					$.ajax({
						type: 'POST',
						data: data,
						url: '/xcx_gw/api/contactUs.php',
						dataType: 'json',
						success: function (msg) {
							console.log(msg)
							if (msg.result < 0) {
								// 有错误信息，弹出提示
								autoTips(msg.param.errorMsg);

							} else if (msg.result > 0) {
								//显示多少人排队
								$('#serve-people-num').text('5')
								$('.serve .serve-select').hide();
								$('.serve .serve-success').show();
								// 操作成功,执行传过来的函数
								
								if(msg.param.errorMsg){autoTips(msg.param.errorMsg)};

							}

						},
						error: function (msg) {
							console.log(msg)
							autoTips('网络异常，请稍后再试！');
						}
					});

				}
				// QQ客服接口
				BizQQWPA.addCustom({
					aty: '0',
					a: '0',
					nameAccount: 800135124,
					selector: 'j-online-qq'
				});
			</script>
