}

.c-50 {
  color: #505050
}

.c-80 {
  color: #808080
}

.img-wrap {
  position: relative;
  overflow: hidden
}

img.center {
  position: absolute;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%)
}

/* input */
input.self {
  font-size: 16px;
  line-height: 16px;
  color: #505050;
