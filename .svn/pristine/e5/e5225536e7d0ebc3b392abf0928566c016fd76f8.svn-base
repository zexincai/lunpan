/* 公共的开始 */
@charset "utf-8";

* {
  margin: 0;
  padding: 0;
}

a,
button {
  cursor: pointer
}

body {
  background: #fff;
  font-family: "思源黑体", Arial;
  color: #000
}

a {
  outline: 0;
  color: inherit;
  text-decoration: none;
}

input,
textarea {
  border: none;
  outline: 0;
  resize: none;
}

p {
  margin: 0;
  word-wrap: break-word
}

h1,
h2,
h3,
h4,
h5 {
  font-weight: 400
}

li,
ul {
  list-style: none
}

dd,
dl,
dt,
li {
  margin: 0
}

fieldset {
  border: none
}

em {
  font-style: normal
}

button,
input[type=button],
input[type=password],
input[type=tel],
input[type=text],
select,
textarea {
  outline: 0;
  vertical-align: middle;
  appearance: none;
  resize: none
}

input[type=radio],
input[type=radio]:focus {
  vertical-align: middle;
  height: 22px !important;
  width: 22px !important;
}

input[type=checkbox] {
  vertical-align: middle;
  height: 14px !important;
  width: 14px !important
}

table {
  border-collapse: collapse;
  border-spacing: 0
}

img {
  border: 0;
  vertical-align: middle;
  display: inline-block;
}

.clear {
  zoom: 1;
  clear: both
}

.clear:after {
  visibility: hidden;
  display: block;
  font-size: 0;
  content: " ";
  clear: both;
  height: 0;
  overflow: hidden
}

.fl {
  float: left
}

.fr {
  float: right
}


/* 头部 */
.header-box {
  box-shadow: 0px 1px 0px 0px rgba(0, 0, 0, 0.1);
}

.header {
  height: 80px;
  max-width: 1200px;
  margin: 0 auto;
  background: #ffffff;
}

.header .header_log {
  margin: 12px 0 12px 50px;
  height: 56px;
}

.header .header_log img {
  width: 56px;
  height: 56px;
  vertical-align: bottom;
}

.header .header_log .log_font {
  font-size: 36px;
  margin-left: 20px;
  display: inline;
  line-height: 56px;
}

.header .header_nav {
  position: relative;
  height: 80px;
  margin-right: 30px;
}

.header_nav ul li {
  display: inline-block;
  position: relative;
  height: 80px;
  font-size: 22px;
  -webkit-transition: all 0.3s;
  transition: all 0.3s
}

.header_nav ul .active {
  border-bottom: 4px solid #08A953;
  background: #EFF3F2;
  box-sizing: border-box;
  font-size: 30px !important;
}

.header_nav ul li>a {
  display: inline-block;
  line-height: 34px;
  padding: 25px 25px 27px 25px;
}

a:hover {
  color: #333333;
  text-decoration: none;
}

/* 头部结束 */
/* 尾部开始 */
.ft {
  height: 90px;
  background: #363839;
  color: #ffffff;
  font-weight: 400;
}

.ft_wrap {
  max-width: 1200px;
  margin: 0 auto;
}

.ft_line {
  font-size: 1.15rem;
  max-width: 642px;
  height: 90px;
  box-sizing: border-box;
  margin: 0 auto;
  padding: 32px 0 35px;
  text-align: center;

}

/* 尾部结束 */
.x-h {
  border-bottom: 1px solid #33333324;
}

/* 公共的结束 */
