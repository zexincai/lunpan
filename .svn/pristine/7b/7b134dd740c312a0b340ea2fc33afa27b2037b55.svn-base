    <script>
      $(document).ready(function () {
        $(".tabitem>a").click(function (e) {
          // 切换不同样式
          $(".tabitem>a").removeClass();
          $(e.target).addClass("active");
          console.log(e)
          var flag = e.target.dataset.cid;
          if (!e.target.dataset.cid) {
            return
          };
          $(".caselist").hide();
          if (flag == "total") {
            $(".casebox .total").show();
          } else if (flag == "mall") {
            $(".casebox .mall").show();
          } else if (flag == "store") {
            $(".casebox .store").show();
          } else if (flag == "restaurant") {
            $(".casebox .restaurant").show();
          } else if (flag == "read") {
            $(".casebox .read").show();
          } else if (flag == "city") {
            $(".casebox .city").show();
          } else if (flag == "display") {
            $(".casebox .display").show();
          }
        })
      })
    </script>
